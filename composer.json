{
    "type": "project",
    "license": "AGPL-3.0-only",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/veelkoov/debris"
        }
    ],
    "require": {
        "php": "8.5.*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "composer/package-versions-deprecated": "*",
        "composer/pcre": "^3.3",
        "doctrine/doctrine-bundle": "^3.1.0",
        "doctrine/doctrine-migrations-bundle": "^4.0.0",
        "doctrine/orm": "*",
        "nette/utils": "^v4.0.5",
        "phpoffice/phpspreadsheet": "^5.3.0",
        "symfony/apache-pack": "^1.0",
        "symfony/browser-kit": "*",
        "symfony/clock": "*",
        "symfony/console": "*",
        "symfony/css-selector": "*",
        "symfony/doctrine-messenger": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/mailer": "*",
        "symfony/messenger": "*",
        "symfony/monolog-bundle": "v4.0.1",
        "symfony/process": "*",
        "symfony/rate-limiter": "*",
        "symfony/runtime": "*",
        "symfony/scheduler": "*",
        "symfony/security-bundle": "*",
        "symfony/security-csrf": "*",
        "symfony/serializer": "*",
        "symfony/twig-bundle": "*",
        "symfony/uid": "*",
        "symfony/validator": "*",
        "symfony/webpack-encore-bundle": "^v2.0.0",
        "symfony/yaml": "*",
        "symfonycasts/verify-email-bundle": "^1.18",
        "twig/cache-extra": "^3.3",
        "twig/extra-bundle": "^3.3",
        "twig/html-extra": "^3.12",
        "veelkoov/debris": "dev-develop"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpdocumentor/reflection-docblock": "*",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^2.1.1",
        "phpstan/phpstan-doctrine": "^2.0.1",
        "phpstan/phpstan-phpunit": "^2.0.3",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0.1",
        "phpunit/phpunit": "^12.1",
        "rector/rector": "*",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "*",
        "symfony/panther": "*",
        "symfony/phpunit-bridge": "*",
        "symfony/stopwatch": "*",
        "symfony/var-dumper": "*",
        "symfony/web-profiler-bundle": "*",
        "vincentlanglet/twig-cs-fixer": "^3.4.0",
        "zenstruck/messenger-test": "^1.11"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "files": [
            "global/functions.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    }
}
